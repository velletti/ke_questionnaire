                    </settings.questionnaires>
                </el>
            </ROOT>
        </basics>
        <layout>
            <ROOT>
                <sheetTitle>LLL:EXT:ke_questionnaire/Resources/Private/Language/locallang_flexforms.xml:sheet.layout
                </sheetTitle>
                <type>array</type>
                <el>
                    <settings.listType>
                        <label>
                            LLL:EXT:ke_questionnaire/Resources/Private/Language/locallang_flexforms.xml:settings.listType
                        </label>
                        <onChange>reload</onChange>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <numIndex index="0">
                                        LLL:EXT:ke_questionnaire/Resources/Private/Language/locallang_flexforms.xml:settings.listType.all
                                    </numIndex>
                                    <numIndex index="1">all</numIndex>
                                </numIndex>
                                <numIndex index="1">
                                    <numIndex index="0">
                                        LLL:EXT:ke_questionnaire/Resources/Private/Language/locallang_flexforms.xml:settings.listType.used
                                    </numIndex>
